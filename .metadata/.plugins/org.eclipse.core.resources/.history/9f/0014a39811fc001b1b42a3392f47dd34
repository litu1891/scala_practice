object typeInterface {
  println("Welcome to the Scala worksheet")       //> Welcome to the Scala worksheet
  val num = 5                                     //> num  : Int = 5
  val decimal = 5.2567                            //> decimal  : Double = 5.2567
  var x = 1 + 2 * 3.5                             //> x  : Double = 8.0
  
}