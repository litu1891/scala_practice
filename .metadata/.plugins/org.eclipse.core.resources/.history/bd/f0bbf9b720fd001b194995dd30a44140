object operatorsExample {
  println("Welcome to the Scala worksheet")       //> Welcome to the Scala worksheet
  var x = 3                                       //> x  : Int = 3
  var y = 9                                       //> y  : Int = 9
  
  var sum = x + y                                 //> sum  : Int = 12
  var b1 = x&y                                    //> b1  : Int = 1
  var b2 = x|y                                    //> b2  : Int = 11
  var b3 = x^y                                    //> b3  : Int = 10
}